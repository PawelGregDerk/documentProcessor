<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <changeSet id="disable-keys" author="Pawel" runAlways="true">
        <sql>SET FOREIGN_KEY_CHECKS=0;</sql>
    </changeSet>
    <changeSet author="Pawel" id="fill_operations">
        <sqlFile path="DML/add_operations.sql" relativeToChangelogFile="true"/>
    </changeSet>
    <changeSet id="enable-keys" author="Pawel" runAlways="true">
        <sql>SET FOREIGN_KEY_CHECKS=1;</sql>
    </changeSet>
</databaseChangeLog>